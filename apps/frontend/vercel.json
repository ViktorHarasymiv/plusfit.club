{
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "https://plusfit-club-backend.onrender.com/auth/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://plusfit-club-backend.onrender.com/:path*"
    },
    {
      "source": "/auth/api/:path*",
      "destination": "https://plusfit-club-backend.onrender.com/:path*"
    },
    {
      "source": "/profile/api/:path*",
      "destination": "https://plusfit-club-backend.onrender.com/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
